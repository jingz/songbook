{
  "name": "Chord Tab",
  "description": "Chord Tab is the App that help to search for chord and tab by Song and Artist from chordtabs.in.th",
  "manifest_version": 2,
  "version": "1.1.1",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "note_16.png",
    "128": "note_128.png"
  },
	"permissions":[
		"http://chordtabs.in.th/*"
  ]
}
